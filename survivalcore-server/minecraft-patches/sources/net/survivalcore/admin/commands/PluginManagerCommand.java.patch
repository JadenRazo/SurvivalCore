+    private boolean executeLookup(CommandSender sender, String[] args) {
+        if (!checkPerm(sender, PermissionNodes.PLUGMAN_LOOKUP)) return true;
+        if (args.length < 2) {
+            sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                .append(Component.text("Usage: /plugman lookup <command>", NamedTextColor.YELLOW)));
+            return true;
+        }
+
+        String commandName = args[1].toLowerCase();
+        SimpleCommandMap commandMap = ((CraftServer) Bukkit.getServer()).getCommandMap();
+        Map<String, Command> knownCommands = commandMap.getKnownCommands();
+
+        Command cmd = knownCommands.get(commandName);
+        if (cmd == null) {
+            sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                .append(Component.text("Command not found: ", NamedTextColor.YELLOW))
+                .append(Component.text(commandName, NamedTextColor.GOLD)));
+            return true;
+        }
+
+        if (cmd instanceof PluginCommand pc) {
+            sender.sendMessage(Component.text("Command ", NamedTextColor.GRAY)
+                .append(Component.text("/" + commandName, NamedTextColor.AQUA))
+                .append(Component.text(" is registered by ", NamedTextColor.GRAY))
+                .append(Component.text(pc.getPlugin().getName(), NamedTextColor.GOLD)));
+        } else {
+            // Check for namespace prefix
+            String owner = "Unknown";
+            for (Map.Entry<String, Command> entry : knownCommands.entrySet()) {
+                if (entry.getValue().equals(cmd) && entry.getKey().contains(":")) {
+                    owner = entry.getKey().split(":")[0];
+                    break;
+                }
+            }
+            sender.sendMessage(Component.text("Command ", NamedTextColor.GRAY)
+                .append(Component.text("/" + commandName, NamedTextColor.AQUA))
+                .append(Component.text(" owner: ", NamedTextColor.GRAY))
+                .append(Component.text(owner, NamedTextColor.GOLD)));
+        }
+
+        return true;
+    }
