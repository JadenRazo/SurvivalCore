+    private boolean executeDump(CommandSender sender) {
+        if (!checkPerm(sender, PermissionNodes.PLUGMAN_DUMP)) return true;
+
+        try {
+            Path dumpFile = Path.of("plugins", "versions.txt");
+            List<String> lines = new ArrayList<>();
+
+            Plugin[] plugins = Bukkit.getPluginManager().getPlugins();
+            Arrays.sort(plugins, Comparator.comparing(Plugin::getName));
+
+            for (Plugin p : plugins) {
+                String status = p.isEnabled() ? "Enabled" : "Disabled";
+                lines.add(p.getName() + " v" + p.getPluginMeta().getVersion() + " - " + status);
+            }
+
+            Files.write(dumpFile, lines);
+
+            sender.sendMessage(Component.text("✓ ", NamedTextColor.GREEN)
+                .append(Component.text("Plugin list exported to ", NamedTextColor.YELLOW))
+                .append(Component.text("plugins/versions.txt", NamedTextColor.GOLD)));
+        } catch (Exception e) {
+            sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                .append(Component.text("Failed to export: ", NamedTextColor.YELLOW))
+                .append(Component.text(e.getMessage(), NamedTextColor.GOLD)));
+        }
+
+        return true;
+    }
