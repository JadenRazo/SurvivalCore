+        try {
+            Plugin loaded = Bukkit.getPluginManager().loadPlugin(pluginFile);
+            if (loaded == null) {
+                sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                    .append(Component.text("Failed to reload plugin. Check console for errors.", NamedTextColor.YELLOW)));
+                return true;
+            }
+
+            loaded.onLoad();
+            Bukkit.getPluginManager().enablePlugin(loaded);
+            craftServer.syncCommands();
+
+            sender.sendMessage(Component.text("✓ ", NamedTextColor.GREEN)
+                .append(Component.text("Reloaded ", NamedTextColor.YELLOW))
+                .append(Component.text(loaded.getName(), NamedTextColor.GOLD)));
+        } catch (Exception e) {
+            sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                .append(Component.text("Failed to reload: ", NamedTextColor.YELLOW))
+                .append(Component.text(e.getMessage(), NamedTextColor.GOLD)));
