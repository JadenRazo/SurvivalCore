+    private boolean executeUsage(CommandSender sender, String[] args) {
+        if (!checkPerm(sender, PermissionNodes.PLUGMAN_USAGE)) return true;
+        if (args.length < 2) {
+            sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                .append(Component.text("Usage: /plugman usage <plugin>", NamedTextColor.YELLOW)));
+            return true;
+        }
+
+        Plugin plugin = Bukkit.getPluginManager().getPlugin(args[1]);
+        if (plugin == null) {
+            sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                .append(Component.text("Plugin not found: ", NamedTextColor.YELLOW))
+                .append(Component.text(args[1], NamedTextColor.GOLD)));
+            return true;
+        }
+
+        SimpleCommandMap commandMap = ((CraftServer) Bukkit.getServer()).getCommandMap();
+        Map<String, Command> knownCommands = commandMap.getKnownCommands();
+
+        List<String> pluginCommands = new ArrayList<>();
+        for (Map.Entry<String, Command> entry : knownCommands.entrySet()) {
+            String key = entry.getKey();
+            Command cmd = entry.getValue();
+
+            if (key.contains(":")) continue;
+
+            if (cmd instanceof PluginCommand pc && pc.getPlugin().equals(plugin)) {
+                pluginCommands.add(key);
+            }
+        }
+
+        pluginCommands.sort(String::compareToIgnoreCase);
+
+        sender.sendMessage(Component.text("═══════════════════════════════════════", NamedTextColor.DARK_GRAY));
+        sender.sendMessage(Component.text("Commands registered by ", NamedTextColor.GRAY)
+            .append(Component.text(plugin.getName(), NamedTextColor.GOLD, TextDecoration.BOLD)));
+        sender.sendMessage(Component.text("═══════════════════════════════════════", NamedTextColor.DARK_GRAY));
+
+        if (pluginCommands.isEmpty()) {
+            sender.sendMessage(Component.text("  No commands registered", NamedTextColor.GRAY));
+        } else {
+            for (String cmd : pluginCommands) {
+                sender.sendMessage(Component.text("  /", NamedTextColor.GRAY)
+                    .append(Component.text(cmd, NamedTextColor.AQUA)));
+            }
+        }
+
+        sender.sendMessage(Component.text("═══════════════════════════════════════", NamedTextColor.DARK_GRAY));
+        return true;
+    }
