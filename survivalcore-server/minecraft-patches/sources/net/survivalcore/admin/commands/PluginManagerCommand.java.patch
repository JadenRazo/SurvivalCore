+    private boolean executeUnload(CommandSender sender, String[] args) {
+        if (!checkPerm(sender, PermissionNodes.PLUGMAN_UNLOAD)) return true;
+        if (args.length < 2) {
+            sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                .append(Component.text("Usage: /plugman unload <plugin>", NamedTextColor.YELLOW)));
+            return true;
+        }
+
+        Plugin plugin = Bukkit.getPluginManager().getPlugin(args[1]);
+        if (plugin == null) {
+            sender.sendMessage(Component.text("✗ ", NamedTextColor.RED)
+                .append(Component.text("Plugin not found: ", NamedTextColor.YELLOW))
+                .append(Component.text(args[1], NamedTextColor.GOLD)));
+            return true;
+        }
+
+        String pluginName = plugin.getName();
+
+        // 1. Disable the plugin
+        if (plugin.isEnabled()) {
+            Bukkit.getPluginManager().disablePlugin(plugin);
+        }
+
+        // 2. Remove plugin commands from the command map
+        CraftServer craftServer = (CraftServer) Bukkit.getServer();
+        SimpleCommandMap commandMap = craftServer.getCommandMap();
+        Map<String, Command> knownCommands = commandMap.getKnownCommands();
+
+        Iterator<Map.Entry<String, Command>> it = knownCommands.entrySet().iterator();
+        while (it.hasNext()) {
+            Map.Entry<String, Command> entry = it.next();
+            Command cmd = entry.getValue();
+
+            if (cmd instanceof PluginCommand pluginCmd) {
+                if (pluginCmd.getPlugin().equals(plugin)) {
+                    cmd.unregister(commandMap);
+                    it.remove();
+                }
+            } else {
+                String key = entry.getKey();
+                if (key.toLowerCase().startsWith(pluginName.toLowerCase() + ":")) {
+                    cmd.unregister(commandMap);
+                    it.remove();
+                }
+            }
+        }
+
+        // 3. Remove from Paper's internal plugin lists
+        try {
+            java.lang.reflect.Field instanceManagerField = Bukkit.getPluginManager().getClass().getDeclaredField("instanceManager");
+            instanceManagerField.setAccessible(true);
+            Object instanceManager = instanceManagerField.get(Bukkit.getPluginManager());
+
+            java.lang.reflect.Field pluginsField = instanceManager.getClass().getDeclaredField("plugins");
+            pluginsField.setAccessible(true);
+            @SuppressWarnings("unchecked")
+            List<Plugin> plugins = (List<Plugin>) pluginsField.get(instanceManager);
+            plugins.remove(plugin);
+
+            java.lang.reflect.Field lookupField = instanceManager.getClass().getDeclaredField("lookupNames");
+            lookupField.setAccessible(true);
+            @SuppressWarnings("unchecked")
+            Map<String, Plugin> lookupNames = (Map<String, Plugin>) lookupField.get(instanceManager);
+            lookupNames.values().removeIf(p -> p.equals(plugin));
+        } catch (Exception e) {
+            sender.sendMessage(Component.text("⚠ ", NamedTextColor.YELLOW)
+                .append(Component.text("Could not fully clean up plugin internals: ", NamedTextColor.GRAY))
+                .append(Component.text(e.getMessage(), NamedTextColor.GOLD)));
+        }
+
+        // 4. Sync commands
+        craftServer.syncCommands();
+        for (Player online : Bukkit.getOnlinePlayers()) {
+            online.updateCommands();
+        }
+
+        // 5. Suggest GC
+        System.gc();
+
+        sender.sendMessage(Component.text("✓ ", NamedTextColor.GREEN)
+            .append(Component.text("Unloaded ", NamedTextColor.YELLOW))
+            .append(Component.text(pluginName, NamedTextColor.GOLD)));
+        return true;
+    }
