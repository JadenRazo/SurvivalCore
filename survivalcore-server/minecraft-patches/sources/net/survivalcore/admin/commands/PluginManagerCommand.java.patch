+    private List<String> getPluginNames(boolean enabledOnly, String prefix) {
+        return Arrays.stream(Bukkit.getPluginManager().getPlugins())
+            .filter(p -> !enabledOnly || p.isEnabled())
+            .filter(p -> enabledOnly || !p.isEnabled())
+            .map(Plugin::getName)
+            .filter(name -> name.toLowerCase().startsWith(prefix.toLowerCase()))
+            .sorted()
+            .collect(Collectors.toList());
+    }
+
+    private List<String> getAllPluginNames(String prefix) {
+        return Arrays.stream(Bukkit.getPluginManager().getPlugins())
+            .map(Plugin::getName)
+            .filter(name -> name.toLowerCase().startsWith(prefix.toLowerCase()))
+            .sorted()
+            .collect(Collectors.toList());
+    }
+
+    private List<String> getUnloadedJars(String prefix) {
+        File pluginsDir = new File("plugins");
+        if (!pluginsDir.exists() || !pluginsDir.isDirectory()) {
+            return List.of();
+        }
+
+        List<String> loadedJars = Arrays.stream(Bukkit.getPluginManager().getPlugins())
+            .filter(p -> p instanceof JavaPlugin)
+            .map(p -> {
+                try {
+                    java.lang.reflect.Method getFileMethod = JavaPlugin.class.getDeclaredMethod("getFile");
+                    getFileMethod.setAccessible(true);
+                    return ((File) getFileMethod.invoke(p)).getName();
+                } catch (Exception e) {
+                    return null;
+                }
+            })
+            .filter(name -> name != null)
+            .collect(Collectors.toList());
+
+        File[] files = pluginsDir.listFiles((dir, name) -> name.endsWith(".jar"));
+        if (files == null) {
+            return List.of();
+        }
+
+        return Arrays.stream(files)
+            .map(File::getName)
+            .filter(name -> !loadedJars.contains(name))
+            .filter(name -> name.toLowerCase().startsWith(prefix.toLowerCase()))
+            .sorted()
+            .collect(Collectors.toList());
+    }
+
+    private List<String> getCommandNames(String prefix) {
+        SimpleCommandMap commandMap = ((CraftServer) Bukkit.getServer()).getCommandMap();
+        Map<String, Command> knownCommands = commandMap.getKnownCommands();
+
+        return knownCommands.keySet().stream()
+            .filter(key -> !key.contains(":"))
+            .filter(key -> key.toLowerCase().startsWith(prefix.toLowerCase()))
+            .sorted()
+            .collect(Collectors.toList());
+    }
