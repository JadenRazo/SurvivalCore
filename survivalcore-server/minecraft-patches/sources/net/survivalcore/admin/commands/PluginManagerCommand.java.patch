+    private boolean checkPerm(CommandSender sender, String perm) {
+        if (sender.hasPermission(perm) || sender.hasPermission(PermissionNodes.PLUGMAN)) {
+            return true;
+        }
+        sender.sendMessage(Bukkit.permissionMessage());
+        return false;
+    }
